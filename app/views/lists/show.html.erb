<h1><%= @list.title %></h1>
<% @bookmarks_needed.each do |bookmark| %>
  <% tool = Tool.find_by(bookmark.tool_id) %>
  <% user = User.find_by(tool.user_id) %>
  <% profile = Profile.find_by(user.id) %>
  <%#might need to change line method %>
  <ul>
    <li><%= tool.title %></li>
    <li><%= tool.description %></li>
    <li><%= tool.rating %></li>
    <li><%= tool.url %></li>
    <li><%= profile.user_name %></li>
  </ul>
<% end %>
